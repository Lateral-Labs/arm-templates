{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "l": {
            "type": "string",
            "defaultValue": "[resourceGroup().location]",
            "allowedValues": [
                "Australia Central",
                "Australia Central 2",
                "Australia East",
                "Australia SouthEast",
                "Brazil South",
                "Canada Central",
                "Canada East",
                "Central India",
                "Central US",
                "East Asia",
                "East US",
                "East US 2",
                "France Central",
                "France South",
                "Japan East",
                "Japan West",
                "Korea Central",
                "Korea South",
                "North Central US",
                "North Europe",
                "South Central US",
                "South India",
                "Southeast Asia",
                "UK South",
                "UK West",
                "West Central US",
                "West Europe",
                "West India",
                "West US",
                "West US 2"
            ],
            "metadata": {
                "description": "Location to be converted to a VSTS available location."
            }
        }
    },
    "variables": {
        "WE": [
            "France Central",
            "France South",
            "North Europe",
            "UK South",
            "UK West",
            "West Europe"
        ],
        "CU": [
            "Central US",
            "East US",
            "East US 2",
            "North Central US",
            "South Central US",
            "West Central US",
            "West US",
            "West US 2"
        ],
        "AE": [
            "Australia Central",
            "Australia Central 2",
            "Australia East",
            "Australia SouthEast"
        ],
        "BS": [
            "Brazil South"
        ],
        "CC": [
            "Canada Central",
            "Canada East"
        ],
        "EA": [
            "East Asia",
            "Japan East",
            "Japan West",
            "Korea Central",
            "Korea South",
            "Southeast Asia"
        ],
        "SI": [
            "Central India",
            "South India",
            "West India"
        ]
    },
    "resources": [],
    "outputs": {
        "location": {
            "type": "string",
            "value": "[if(contains(variables('WE'), parameters('l')), 'West Europe', if(contains(variables('CU'), parameters('l')), 'Central US', if(contains(variables('AE'), parameters('l')), 'Australia East', if(contains(variables('BS'), parameters('l')), 'Brazil South', if(contains(variables('EA'), parameters('l')), 'East Asia', if(contains(variables('SI'), parameters('l')), 'South India', 'Canada Central'))))))]"
        }
    }
}